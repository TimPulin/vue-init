{
  "name": "olimpia",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "preview": "vite preview",
    "fixcss": "npx stylelint '**/*.scss' --fix",
    "prepare": "husky install"
  },
  "lint-staged": {
    "*.{css, scss}": "stylelint --fix"
  },
  "dependencies": {
    "@vue/eslint-config-airbnb": "^8.0.0",
    "axios": "^1.6.7",
    "eslint-import-resolver-custom-alias": "^1.3.2",
    "mkdirp": "^3.0.1",
    "optionator": "^0.9.3",
    "pinia": "^2.1.7",
    "prettier": "^3.2.5",
    "sass": "^1.71.1",
    "vue": "^3.4.19",
    "vuetify": "^3.5.6"
  },
  "devDependencies": {
    "@vitejs/plugin-vue": "^5.0.4",
    "eslint": "^8.56.0",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.1.3",
    "eslint-plugin-vue": "^9.21.1",
    "husky": "^8.0.0",
    "lint-staged": "^15.2.2",
    "stylelint": "^16.2.1",
    "stylelint-config-sass-guidelines": "^11.0.0",
    "stylelint-config-standard-scss": "^13.0.0",
    "stylelint-order": "^6.0.4",
    "vite": "^5.1.4",
    "vite-plugin-eslint": "^1.8.1"
  },
  "resolve-path-alias": {
    "alias": {
      "@/*": "src/*",
      "@styles/*": "src/assets/styles/*",
      "@img/*": "src/assets/img/*"
    }
  },
  "eslintConfig": {
    "extends": [
      "airbnb-base"
    ],
    "plugins": [
      "import"
    ],
    "parserOptions": {
      "sourceType": "module"
    }
  }
}
